{
  "manifest_version": 2,
  "name": "Dynamic page clipper",
  "version": "1.0.0",
  "description": "Create window that shows part of any web page dynamically.",
  "author": "tkrkt",
  "background": {
    "scripts": [
      "index.js"
    ],
    "persistent": false
  },
  "browser_action": {
  },
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "tabCapture",
    "activeTab",
    "contextMenus"
  ]
}